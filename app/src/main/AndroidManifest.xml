        <activity
            android:name="com.qr.girish.qramazeon.ProductActivity"
            android:label="@string/title_activity_product_activty" >
        </activity>
        <activity
            android:name="com.qr.girish.qramazeon.CartActivity"
            android:label="@string/title_activity_cart" >
        </activity>
        <activity
            android:name="com.qr.girish.qramazeon.LoadCart"
            android:label="@string/title_activity_load_cart" >
        </activity>
        <activity
            android:name="com.qr.girish.qramazeon.TimeActivity"
            android:label="Time Activity" />
<activity
